    border: 1px solid rgba(0, 0, 0, 0);
    border-radius: 0;
    margin-bottom: 18px;
    padding: 15px;    
}
.error {
    background: none repeat scroll 0 0 #FFCCCC;
    border: 1px solid #990000;
    font-size: 100%;
    margin: 0 0 10px;
    padding: 10px;
    font-weight: bold;
}
.error ol {
